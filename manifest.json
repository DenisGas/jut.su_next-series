{
    "name": "jut.su Next-Series",
    "description": "Extension for jut.su - which makes video auto-play, auto-skip anime intro, and auto-open next series after the end series.",
    "version": "2.2.2",
    "manifest_version": 3,
    "icons": {

      "16":"./icons/16.png",
      "32":"./icons/32.png",
      "48":"./icons/48.png",
      "128":"./icons/128.png"
    },
    "action": {
      "default_popup":"popup.html"
    },
    "permissions": ["storage"],
    "content_scripts": [
        {
          "js": ["./main.js"],
          "matches": [
            "https://jut.su/*"
          ]
        }
      ],
    "background":{
    }
}